Package: multidplyr
Title: A Multi-Process 'dplyr' Backend
Version: 0.1.0.9000
Authors@R: c(
    person("Hadley", "Wickham", , "hadley@rstudio.com", role = c("aut", "cre")),
    person("RStudio", role = "cph")
